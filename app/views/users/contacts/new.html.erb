<div class="container">
  <div class="row">
    <div class="mx-auto">
      <h1 class="my-5">お問い合わせ</h1>
      <%= form_with model: @contact, url:users_contacts_path, local:true do |f| %>
        <h4>タイトル</h4>
        <%= f.text_field :title %>
        <h4>本文</h4>
        <%= f.text_area :massage %>
        <%= f.submit '投稿' %>
        <% end %>
        <%= link_to "一覧を見る", users_contacts_path, class:"btn btn-success" %>
    </div>
  </div>
</div>
<h1>Book編集</h1>
<%= form_with model: @book, url:"/admins/books/#{@book.id}/edit", local:true do |f| %>
  <h4>商品画像</h4>
  <%= f.attachment_field :image %>
  <h4>タイトル</h4>
  <%= f.text_field :name %>
  <h4>本文</h4>
  <%= f.text_area :descript %>
  <h4>ジャンル</h4>
  <%= f.collection_select :genre_id, Genre.all ,:id, :name, include_blank: "--選択してください--" %>
  
  <%= f.submit '投稿' %>
<% end %>
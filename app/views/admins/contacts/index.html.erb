<h1>お問い合わせ一覧</h1>

<% @contacts.each do |contact| %>
  <p>ユーザー名</p>
  <span><%= contact.user.name %></span>
  <p>タイトル</p>
  <span><%= contact.title %></span>
  <p>本文</p>
  <span><%= contact.massage %></span>
  <%= link_to "削除", users_contact_path(contact.id), method: :delete, class:"btn btn-danger" %>
<% end %>